<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FeedbackSystem2K17PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>beans.CourseBean</class>
    <class>beans.LoginBean</class>
    <class>beans.SubjectBean</class>
    <class>beans.FeedbackBean</class>
    <class>beans.CsfBean</class>
    <class>beans.FacultyBean</class>
    <class>beans.SchedulerBean</class>
    <class>beans.DepartmentBean</class>
    <class>beans.VariableBean</class>
    <class>beans.ClassBean</class>
    <class>beans.SessionBean</class>
    <class>beans.ScheduledFeedbackBean</class>
    <class>beans.SectionAFeedback</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/feedback_system_old?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
